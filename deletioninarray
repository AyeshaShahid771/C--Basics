#include <iostream>
using namespace std;
/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main() {
   int pos,ele;
   int arr[5] = {1,4,6,3,2}	;
   cout<<"enter  the  position where you want to delete number ="<<endl;
   cin>>pos;
   
 
   for(int i = pos -1; i < 5; i++){
   	arr[i] = arr[i +1];
   	
   }
	

	
	for(int i =0; i<4; i++){
		cout<<arr[i] <<" ";
	}
	
	
	
	
	
	
	return 0;
}
