#include <iostream>
using namespace std;

/* run this program using the console pauser or add your own getch, system("pause") or input loop */

int main() 
{
 
	int number;
	cout <<"Enter the number!"<<endl;
	cin>>number;
	int factorial=1;
	for(int i=1;i<=number;i++)
	{
		cout<<i<<endl;
		factorial=factorial*i;
		
	}
	cout<<factorial;
	
	
	
	return 0;
}
